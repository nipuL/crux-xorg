# Description: xorg proto kbproto
# URL: http://xorg.freedesktop.org
# Maintainer: Tilman Sauerbeck, tilman at crux dot nu

name=xorg-kbproto
version=1.0.2
release=1
source=(http://xorg.freedesktop.org/releases/X11R7.0/src/everything/kbproto-$version.tar.bz2)

build() {
	cd kbproto-$version

	./configure --prefix=/usr

	make
	make DESTDIR=$PKG install
}
