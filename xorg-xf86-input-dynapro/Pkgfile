# Description: xorg driver xf86-input-dynapro
# URL: http://xorg.freedesktop.org
# Maintainer: Tilman Sauerbeck, tilman at crux dot nu

name=xorg-xf86-input-dynapro
version=1.0.0.5
release=1
source=(http://xorg.freedesktop.org/releases/X11R7.0/src/everything/xf86-input-dynapro-$version.tar.bz2)

build() {
	cd xf86-input-dynapro-$version

	./configure --prefix=/usr

	make
	make DESTDIR=$PKG install
}
