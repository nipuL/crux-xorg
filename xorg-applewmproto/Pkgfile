# Description: xorg proto applewmproto
# URL: http://xorg.freedesktop.org
# Maintainer: Tilman Sauerbeck, tilman at crux dot nu

name=xorg-applewmproto
version=1.0.3
release=1
source=(http://xorg.freedesktop.org/releases/X11R7.0/src/everything/applewmproto-$version.tar.bz2)

build() {
	cd applewmproto-$version

	./configure --prefix=/usr

	make
	make DESTDIR=$PKG install
}
