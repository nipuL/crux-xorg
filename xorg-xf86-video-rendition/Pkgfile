# Description: xorg driver xf86-video-rendition
# URL: http://xorg.freedesktop.org
# Maintainer: Tilman Sauerbeck, tilman at code-monkey dot de

name=xorg-xf86-video-rendition
version=4.0.1.3
release=1
source=(http://xorg.freedesktop.org/releases/X11R7.0/src/everything/xf86-video-rendition-$version.tar.bz2)

build() {
	cd xf86-video-rendition-$version

	./configure --prefix=/usr

	make
	make DESTDIR=$PKG install
}
