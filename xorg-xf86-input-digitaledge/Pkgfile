# Description: xorg driver xf86-input-digitaledge
# URL: http://xorg.freedesktop.org
# Maintainer: Tilman Sauerbeck, tilman at code-monkey dot de

name=xorg-xf86-input-digitaledge
version=1.0.1.3
release=1
source=(http://xorg.freedesktop.org/releases/X11R7.0/src/everything/xf86-input-digitaledge-$version.tar.bz2)

build() {
	cd xf86-input-digitaledge-$version

	./configure --prefix=/usr

	make
	make DESTDIR=$PKG install
}
